<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>EDIT PROJECT</title>
  </head>
  <body>
    <form class="" action="/poject/update/data[0].id" method="post">
      <fieldset>
        <input type="text" name="name" value="data[0].name" placeholder="nama">
        <input type="text" name="status" value="data[0].status" placeholder="status">
        <section>
          <%dataSupervisor.forEach(z=>{%>
            <%if(z.id == data[0].id){%>
              <option value="<%=z.id%>"selected><%=z.name%></option>
            <%}else{%>
              <option value="<%=z.id%>"><%=z.name%></option>
            <%}%>
          <%})%>
        </section>
      </fieldset>
    </form>
    <a href="/project/list"></a>
  </body>
</html>
